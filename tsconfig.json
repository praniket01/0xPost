{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "app/components/Dataverse/CreateUser.jsx", "app/components/Dataverse/LoadPostById.jsx", "app/(site)/profile/[profile]/page.jsx", "app/components/providers.tsx", "app/(site)/posts/[post]/page.jsx", "app/components/Subscribers.jsx", "app/(site)/praniket/page.jsx", "app/components/Dataverse/Post.jsx"],
  "exclude": ["node_modules"]
}